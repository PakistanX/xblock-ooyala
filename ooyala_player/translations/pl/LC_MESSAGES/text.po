#: ooyala_player/ooyala_player.py:291 ooyala_player/ooyala_player.py:514
msgid "Display Name"
msgstr "Wyświetlana nazwa"

#: ooyala_player/ooyala_player.py:292 ooyala_player/ooyala_player.py:515
msgid "This name appears in the horizontal navigation at the top of the page."
msgstr "Nazwa ta pojawi się w pasku horyzontalnym nawigacji u góry strony."

