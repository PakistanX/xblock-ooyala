msgstr "Klucz 3play api"

#: ooyala_player/ooyala_player.py:362 ooyala_player/ooyala_player.py:585
msgid "3play Api Key for transcript."
msgstr "Klucz 3play api do transkryptów"

#: ooyala_player/ooyala_player.py:368 ooyala_player/ooyala_player.py:591
#: ooyala_player/templates/html/ooyala_player_edit.html:76
msgid "Player Width"
msgstr "Szerokość odtwarzacza"

#: ooyala_player/ooyala_player.py:369 ooyala_player/ooyala_player.py:592
msgid "The width of the player in pixels."
msgstr "Szerokość odtwarzacza wyrażona w pikselach."

#: ooyala_player/ooyala_player.py:375 ooyala_player/ooyala_player.py:598
#: ooyala_player/templates/html/ooyala_player_edit.html:83
msgid "Player Height"
msgstr "Wysokość odtwarzacza"

#: ooyala_player/ooyala_player.py:376 ooyala_player/ooyala_player.py:599
msgid "The height of the player in pixels."
msgstr "Wysokość odtwarzacza wyrażona w pikselach."

#: ooyala_player/ooyala_player.py:382 ooyala_player/ooyala_player.py:605
#: ooyala_player/templates/html/ooyala_player_edit.html:90
msgid "Expiration Time"
msgstr "Czas wygaśnięcia"

#: ooyala_player/ooyala_player.py:383 ooyala_player/ooyala_player.py:606
#: ooyala_player/templates/html/ooyala_player_edit.html:93
msgid "Expiration time in seconds. Needed to generate a player token."
msgstr "Czas wygaśnięcia wyrażony w sekundach. Wymagany, aby wygenerować token odtwarzacza."

#: ooyala_player/ooyala_player.py:389 ooyala_player/ooyala_player.py:612
msgid "Fire Progress Event on Student View"
msgstr "Fire Progress Event w widoku ucznia"

#: ooyala_player/ooyala_player.py:390 ooyala_player/ooyala_player.py:613
msgid ""
"Set to True if you would like to get a progress event in the event stream "
"when the user views this xBlock."
msgstr "Ustaw na true, jeśli chcesz uzyskać parametr progress event w strumieniu wydarzeń, kiedy użytkownik ogląda ten xBlock."

